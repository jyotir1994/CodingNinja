    "def String_to_Integer(n,si):\n",
    "    if(si==0):\n",
    "        return int(n[si])\n",
    "    smalloutput=int(n[si])+10*String_to_Integer(n,si-1)\n",
    "    return smalloutput\n",
    "a=input()\n",
    "n=String_to_Integer(a,len(a)-1)\n",
    "print(n)"
   ]
