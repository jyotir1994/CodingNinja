    "from sys import setrecursionlimit\n",
    "setrecursionlimit(10**6) \n",
    "\n",
    "def mul(m, n):\n",
    "\n",
    "    if m == 0 or n == 0:\n",
    "        return 0\n",
    " \n",
    "    if n == 1:\n",
    "        return m\n",
    " \n",
    "    if m == 1:\n",
    "        return n\n",
    " \n",
    "    return m + mul(m, n - 1)\n",
    "\n",
    "m = int(input())\n",
    "n = int(input())\n",
    "\n",
    "print(mul(m,n))"
   ]
  }
