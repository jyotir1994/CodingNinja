    "def powerOfNum(x, n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    return (x*powerOfNum(x, n-1))\n",
    "\n",
    "str = input().split()\n",
    "x, n = int(str[0]), int(str[1])\n",
    "print(powerOfNum(x,n))"
   ]
